<Window x:Class="Laboratory.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laboratory"
        mc:Ignorable="d"
        WindowStyle="None"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="White">

        <TextBlock HorizontalAlignment="Center" Margin="0,92,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="260" FontSize="25" Height="45" FontWeight="Bold" FontFamily="Hobo Std" Foreground="#FFAA2BCC"><Run Text="Меню"/><Run Text=" Лаболатори"/><Run Language="ru-ru" Text="и"/><LineBreak/><Span FontSize="18.666666666666664"/></TextBlock>
        <Button Click="ToTable_Click" Content="Информация" HorizontalAlignment="Left" Margin="270,159,0,0" VerticalAlignment="Top" Height="46" Width="245" Background="#FF9D86B5" FontSize="22" Foreground="White"/>
        <Button Click="Out_Click" Content="Выйти" HorizontalAlignment="Left" Margin="270,225,0,0" VerticalAlignment="Top" Height="46" Width="245" Background="#FF9D86B5" FontSize="22" Foreground="White"/>

        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>
        
    </Grid>
</Window>
